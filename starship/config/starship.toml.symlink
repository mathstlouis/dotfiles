# Don't print a new line at the start of the prompt
add_newline = false


# Disable the package module, hiding it from the prompt completely
[package]
disabled = true

[directory]
truncation_length = 3

[memory_usage]
disabled = false
threshold = -1
symbol = " "
style = "bold dimmed green"

[env_var]
variable = "GOOGLE_APPLICATION_CREDENTIALS"
default = "empty GAC"
style = "fg:green bg:black"

[kubernetes]
symbol = "â›µ "
style = "bold blue"
disabled = false

[kubernetes.context_aliases]
"gke_devl-gke-230115_northamerica-northeast1_dev1" = "dev1"
"gke_prod-gke-230115_northamerica-northeast1_prod1" = "prod1"
